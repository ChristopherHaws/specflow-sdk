<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <!-- This ensures that msbuild will rebuild this project if the props/targets file changes -->
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="**/*$(DefaultSpecFlowGeneratedFeatureExtension)" />
  </ItemGroup>

  <ItemGroup Condition=" '$(UsingMicrosoftNETSdk)' == 'true' AND '$(EnableDefaultSpecFlowItems)' == 'true' ">
    <SpecFlowFeatureFile Include="**/*$(DefaultSpecFlowFeatureExtension)" Exclude="$(DefaultItemExcludes);$(DefaultExcludesInProjectFolder)" />
    <UpToDateCheckInput Include="@(SpecFlowFeatureFile)" />
    <UpToDateCheckOutput Include="@(SpecFlowFeatureFile->'$(SpecFlowIntermediateOutputPath)%(RecursiveDir)%(Filename)%(Extension).g.cs')" />
  </ItemGroup>

</Project>
