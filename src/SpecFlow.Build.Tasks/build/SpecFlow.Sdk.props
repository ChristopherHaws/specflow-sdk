<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <!-- This ensures that msbuild will rebuild this project if the props/targets file changes -->
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <PropertyGroup>
    <SpecFlowBuildTasksDirectory>$(MSBuildThisFileDirectory)..\tools\</SpecFlowBuildTasksDirectory>
    <SpecFlowBuildTasksAssembly>$(SpecFlowBuildTasksDirectory)Arvato.Automation.SpecFlow.MsBuild.dll</SpecFlowBuildTasksAssembly>
  </PropertyGroup>

  <!-- Default item includes (globs and implicit references) -->
  <Import Project="SpecFlow.Sdk.DefaultItems.props" />

</Project>